{
  "scripts": {
    "client": "run-p watch:server watch:client open:client",
    "graphiql": "run-p watch:server open:graphiql",
    "info": "npm-scripts-info",
    "open:client": "opn http://localhost:3001/",
    "open:graphiql": "opn http://localhost:3000/graphql",
    "sync-schema": "cd server && babel-node src/sync-schema.js",
    "watch:client": "cd client && webpack-dev-server",
    "watch:server": "cd server && nodemon src/start.js"
  },
  "scripts-info": {
    "client": "Starts the server and the client and opens the client in the browser.",
    "graphiql": "Starts the server and opens the GraphiQL in the browser.",
    "info": "Print this info.",
    "open:client": "Opens the client in the browser.",
    "open:graphiql": "Opens GraphiQL in the browser.",
    "sync-schema": "Generate the schema.gql file from the schema defined via the JS API.",
    "watch:client": "Starts the client and watches for source file changes.",
    "watch:server": "Starts the server and watches for source file changes."
  },
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.2",
    "express-graphql": "^0.6.3",
    "graphql": "^0.9.1",
    "html-webpack-plugin": "^2.29.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.6",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "npm-scripts-info": "^0.3.6",
    "opn-cli": "^3.1.0",
    "react": "^15.6.1",
    "react-apollo": "^1.4.3",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.24.0",
    "style-loader": "^0.18.2",
    "typeface-roboto": "^0.0.31",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1"
  }
}
